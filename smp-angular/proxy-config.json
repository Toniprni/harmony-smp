{
  "/rest/**": {
    "target" : "https://niis-smp.lxd:8443/ui/",
    "secure" : "false",
    "pathRewrite": {
      "^/rest": "/smp/ui/rest"
    },
    "logLevel": "debug",
    "changeOrigin": true,
    "cookiePathRewrite": {
      "/smp": "/"
    },
    "cookieDomainRewrite": "localhost"
  },
  "/public/**": {
    "target" : "https://niis-smp.lxd:8443/ui/",
    "secure" : "false",
    "pathRewrite": {
      "^/public": "/smp/ui/public"
    },
    "logLevel": "debug",
    "changeOrigin": true,
    "cookiePathRewrite": {
      "/smp": "/"
    },
    "cookieDomainRewrite": "localhost"
  },
  "/internal/**": {
    "target" : "https://niis-smp.lxd:8443/ui/",
    "secure" : "false",
    "pathRewrite": {
      "^/internal": "/smp/ui/internal"
    },
    "logLevel": "debug",
    "changeOrigin": true,
    "cookiePathRewrite": {
      "/smp": "/"
    },
    "cookieDomainRewrite": "localhost"
  }
}
