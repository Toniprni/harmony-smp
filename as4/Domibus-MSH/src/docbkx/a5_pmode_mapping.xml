                    <row>
                        <entry>Endpoint</entry>
                        <entry>
                            maps to PMode[1].Protocol.Address
                        </entry>
                        <entry>
                            <link xlink:href="http://docs.oasis-open.org/ebxml-msg/ebms/v3.0/core/os/ebms_core-3.0-spec-os.html#4.3.2.PMode[1].Protocol|outline"
