			<groupId>eu.europa.ec.digit.domibus</groupId>
			<artifactId>domibus-entity</artifactId>
			<version>${domibus.app.version}</version>
		</dependency>

		<dependency>
			<groupId>eu.europa.ec.digit.domibus</groupId>
			<artifactId>domibus-domain</artifactId>
