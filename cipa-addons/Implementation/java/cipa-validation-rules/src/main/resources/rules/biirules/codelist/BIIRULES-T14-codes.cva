<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ContextValueAssociation name="BIIRULES-T14-codes" xmlns:ns2="http://docs.oasis-open.org/codelist/ns/ContextValueAssociation/1.0/">
  <ValueLists>
    <ValueList uri="CountryIdentificationCode.gc" xml:id="CountryIdentificationCode" />
    <ValueList uri="CurrencyCode.gc" xml:id="CurrencyCode" />
    <ValueList uri="TaxCategoryID.gc" xml:id="TaxCategoryID" />
    <ValueList uri="TaxSchemeID.gc" xml:id="TaxSchemeID" />
  </ValueLists>
  <Contexts>
    <Context address="cbc:DocumentCurrencyCode" mark="fatal" values="CurrencyCode">
      <Message>[CL-014-001]-DocumentCurrencyCode MUST be coded using ISO code list 4217</Message>
    </Context>
    <Context address="@currencyID" mark="fatal" values="CurrencyCode">
      <Message>[CL-014-002]-currencyID MUST be coded using ISO code list 4217</Message>
    </Context>
    <Context address="cac:Country/cbc:IdentificationCode" mark="fatal" values="CountryIdentificationCode">
      <Message>[CL-014-003]-Country codes in a credit note MUST be coded using ISO code list 3166-1</Message>
    </Context>
    <Context address="cac:TaxScheme/cbc:ID" mark="warning" values="TaxSchemeID">
      <Message>[CL-014-004]-Credit Note tax schemes MUST be coded using UN/ECE 5153 code list</Message>
    </Context>
    <Context address="cac:TaxCategory/cbc:ID" mark="warning" values="TaxCategoryID">
      <Message>[CL-014-005]-Credit Note tax categories MUST be coded using UN/ECE 5305 code list</Message>
    </Context>
  </Contexts>
</ns2:ContextValueAssociation>
